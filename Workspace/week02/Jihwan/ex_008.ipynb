    "def sotssu(n):\n",
    "    p = 2\n",
    "    while p < n:\n",
    "        if n%p == 0:\n",
    "            break\n",
    "        p += 1\n",
    "\n",
    "    if p == n:\n",
    "        print(f'{n} is sotssu')\n",
    "    else:\n",
    "        print(f'{n} is not sotssu')\n",
    "\n",
    "\n",
    "n = 2\n",
    "while n <= 20:\n",
    "    sotssu(n)\n",
    "    n += 1"
   ]
  }
