    "def sotssu(n):\n",
    "    c = 2\n",
    "    while c <= n:\n",
    "        if n%c == 0:\n",
    "            break\n",
    "        else:\n",
    "            c += 1\n",
    "        \n",
    "    if c != n:\n",
    "        print(f'{n} is hapsungsu')\n",
    "    else:\n",
    "        print(f'{n} is sotssu')\n",
    "\n",
    "n = int(input())\n",
    "sotssu(n)"
   ]
  }
